project Day_2 is

   for Source_Dirs use ("src/**");
   for Object_Dir use ".obj";

   for Main use project'Main & ("day_2-main_2.adb");
   for Main use project'Main & ("day_2-main_1.adb");

   package Compiler is
      for Switches ("ada") use ("-gnatX", "-gnatf", "-gnatwa");
   end Compiler;

end Day_2;

